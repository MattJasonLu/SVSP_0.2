<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jdlink.mapper.WasteInclusionTypeMapper">

    <select id="getByQuestionnaireId" parameterType="String" resultType="WasteInclusionType">
        select wasteInclusionTypeId from r_questionnaireAndWasteInclusionType WHERE questionnaireId=#{questionnaireId}
    </select>
</mapper>